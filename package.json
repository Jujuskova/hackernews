{
  "name": "hackernews",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "get-graphql-schema": "^2.1.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-relay": "^6.0.0",
    "react-router-dom": "^5.1.1",
    "react-scripts": "3.1.2",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
